{
    "name": "raysoft/yii2-wechat",
    "type": "yii2-extension",
    "description": "适用于Yii2的微信接口模块。原作者不继续更新了，fork后维护使用。",
    "keywords": ["yii2", "wechat", "weixin", "module"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/jesonray/yii2-wechat/issues",
        "wiki": "https://github.com/jesonray/yii2-wechat/wiki",
        "source": "https://github.com/jesonray/yii2-wechat"
    },
    "authors": [
        {
            "name": "CallMeZ",
            "email": "callme-z@qq.com"
        }
    ],
    "require": {
        "yiisoft/yii2": ">=2.0.x-dev",
        "yiisoft/yii2-bootstrap": "*",

        "symfony/yaml": "*",

        "callmez/yii2-wechat-sdk" : "*",

        "kartik-v/yii2-grid": "~3.0",
        "kartik-v/yii2-mpdf": "~1.0",
        "kartik-v/yii2-widgets": "~3.0",

        "bower-asset/react": "~0.13",
        "bower-asset/jquery.fileapi": "~0.4",
        "bower-asset/angular": "1.3.*@stable",
        "bower-asset/angular-drag-and-drop-lists": "1.*"
    },
    "autoload": {
        "psr-4": {"raysoft\\wechat\\": ""}
    },
    "extra": {
        "bootstrap": "raysoft\\wechat\\Bootstrap"
    }
}